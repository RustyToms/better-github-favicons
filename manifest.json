{
  "manifest_version": 2,
  "name": "Better Github Favicons",
  "description": "Indicate PR status with colored favicons",
  "version": "0.2.0",
  "web_accessible_resources": [
    "icons/pr.ico",
    "icons/pr_fail.ico",
    "icons/pr_merged.ico",
    "icons/pr_pass.ico"
  ],
  "icons": {
     "16": "logos/16.png",
     "48": "logos/48.png",
    "128": "logos/128.png",
    "512": "logos/512.png"
  },
  "content_scripts": [{
    "matches": ["https://github.com/*"],
    "js": ["jquery-1.9.1.min.js", "favicon.js"],
    "run_at": "document_idle"
  }]
}
