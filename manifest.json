{
  "manifest_version": 2,
  "name": "Better Github Favicons",
  "description": "Indicate PR status with colored favicons",
  "version": "0.1",
  "web_accessible_resources": [
    "icons/pr.ico",
    "icons/pr_fail.ico",
    "icons/pr_pass.ico"
  ],
  "content_scripts": [{
    "matches": ["https://github.com/*"],
    "js": ["jquery-1.9.1.min.js", "favicon.js"],
    "run_at": "document_idle"
  }]
}
