{
  "manifest_version": 2,
  "name": "Better Github Favicons",
  "description": "Indicate PR status with colored favicons",
  "version": "0.1",
  "background": {
    "page": "./background.html"
  },
  "options_page": "./options.html",
  "content_scripts": [{
    "matches": ["https://*/*"],
    "js": ["jquery-1.9.1.min.js", "favicon.js"],
    "run_at": "document_idle"
  }]
}
